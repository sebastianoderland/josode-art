{
    "scripts": {
        "install-all": "npm i && cd backend && npm i && cd ../frontend && npm i",
        "start-backend": "cd backend && npm run start",
        "start-frontend": "cd frontend && npm run start",
        "start": "npm-run-all --parallel start-backend start-frontend"
    },
    "dependencies": {
        "npm-run-all": "^4.1.5"
    }
}
